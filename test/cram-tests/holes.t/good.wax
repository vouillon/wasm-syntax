
fn f (i32) -> i32 {
  1; 2; 3;  _ + (_ + _);
}


fn g () -> i32 {
  1; 2;  _ + (_ + 3);
}

type t = {x: i32, y : i32}

fn h () -> &t {
  1; {t| x: _, y:1};
}

fn i () -> &t {
  1; {t| y:1, x: _};
}

fn j () -> i32 {
  1; f(_);
}

fn k (x: i32) -> i32 {
  2; x?_:1
}

fn l (x: i32) -> i32 {
  1; 2; x?_:_
}
